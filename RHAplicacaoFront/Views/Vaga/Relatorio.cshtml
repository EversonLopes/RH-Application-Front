@model IEnumerable<RHAplicacaoFront.Models.RelatorioVaga>

@{
    ViewBag.Title = "Relatório de Vagas";
}

<br />
<h2>Relatório de Vagas</h2>
<br />
<br />

<p>
    <a href="@Url.Action("Index", "Vaga")" class="btn btn-primary btn-xs">
        <span class="glyphicon glyphicon-arrow-left" aria-hidden="true"></span>
        Voltar
    </a>
</p>
<br />
<table class="relatorio">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Candidato.Nome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Vaga.DescricaoVaga)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.percentual) %
            </th>
        </tr>
    </thead>
    <tbody>
        @if (Model.Count() > 0)
        {
            foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Candidato.Nome)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Vaga.DescricaoVaga)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.percentual)
                    </td>

                </tr>
            }
        }
    </tbody>

</table>
